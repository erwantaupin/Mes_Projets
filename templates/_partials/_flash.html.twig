{% for label, flash in app.flashes %}
	{% for message in flash %}
		<div class="alert alert-{{ label }} alert-dismissible fade show m-0" role="alert">
			<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
			<div class=" alert-message">
				{{message|raw}}
			</div>
		</div>
	{% endfor %}
{% endfor %}
